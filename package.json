{
  "name": "cubid-wallet",
  "version": "0.0.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:css",
    "build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "concurrently \"npm run storybook:css\" \"storybook dev -p 6006\"",
    "storybook:css": "tailwindcss -w -i ./src/tailwind-entry.css -o ./src/index.css",
    "build-storybook": "concurrently \"npm run build-storybook:css\" \"storybook build\"",
    "build-storybook:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./src/index.css",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.0.9",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.7",
    "@near-wallet-selector/core": "^8.9.15",
    "@near-wallet-selector/here-wallet": "^8.9.15",
    "@near-wallet-selector/ledger": "^8.9.15",
    "@near-wallet-selector/modal-ui": "^8.9.15",
    "@near-wallet-selector/my-near-wallet": "^8.9.15",
    "@near-wallet-selector/near-wallet": "^8.9.3",
    "@near-wallet-selector/sender": "^8.9.15",
    "@rainbow-me/rainbowkit": "^2.2.3",
    "@tanstack/react-query": "^5.64.2",
    "assert": "^2.1.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "cbor-web": "^10.0.3",
    "crypto-browserify": "^3.12.1",
    "cubid-sdk": "^1.8.6",
    "http": "0.0.1-security",
    "lucide-react": "^0.474.0",
    "near-api-js": "^5.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^6.0.11",
    "vm-browserify": "^1.1.2",
    "wagmi": "^2.14.9"
  }
}